{
    "ul-indent": {
        "indent": 4
    },
    "line-length": {
        "stern": true,
        "line_length": 120
    },
    "no-inline-html": {
        "allowed_elements": [
            "a",
            "details",
            "summary"
        ]
    },
    "ul-style": {
        "style": "dash"
    },
    "blanks-around-fences": {
        "list_items": false
    },
    "emphasis-style": {
        "style": "asterisk"
    }
  }
